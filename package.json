{
  "name": "vango",
  "version": "1.0.0",
  "description": "react framework",
  "main": "src/index.tsx",
  "repository": {
    "type": "git",
    "url": "https://github.com/ss-xiong/Vango"
  },
  "author": "ss-xiong<tonyxiong.xss@outlook.com>",
  "license": "GNU",
  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=dev webpack-dev-server --config webpack.config.js --colors",
    "build:beta": "cross-env NODE_ENV=beta webpack --config webpack.config.js --colors",
    "build:prod": "cross-env NODE_ENV=prod webpack --config webpack.config.js --colors",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "lint:ts": "eslint src/**/*.tsx --no-fix",
    "lint:style": "stylelint src/**/*.scss --no-fix"
  },
  "keywords": [
    "van goha",
    "react framework"
  ],
  "dependencies": {
    "prop-types": "^15.7.2",
